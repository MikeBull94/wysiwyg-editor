{
  "name": "wysiwyg-editor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/*",
    "check-updates": "ncu",
    "update-dependencies": "ncu --upgrade && npm install",
    "dev": "webpack-dev-server --inline --hot --open",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "font-awesome": "4.6.3",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "slate": "0.13.6",
    "slate-auto-replace": "0.1.0",
    "slate-auto-replace-text": "0.3.1",
    "slate-edit-code": "0.3.0"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "cross-env": "3.0.0",
    "css-loader": "0.25.0",
    "json-loader": "0.5.4",
    "node-sass": "3.10.0",
    "npm-check-updates": "2.8.0",
    "sass-loader": "4.0.2",
    "style-loader": "^0.13.0",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1"
  }
